<script src="/javascripts/jquery-1.3.2.min.js"></script>
</script><script src="/faye.js"></script>

<script type="text/javascript">
      var client = new Faye.Client('/faye', {
          timeout: 120
      });
      
</script> 

<div id="messages" ></div>

<script type="text/javascript">
      client.subscribe('/debate', function(debate) {
        $("#messages").append("<p>"+ debate.text0.username + " said: <br/>re:" + debate.text1.title + " <br/> " + debate.text2.content + "</p>");
      });
</script>

